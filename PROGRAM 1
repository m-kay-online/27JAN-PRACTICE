/*Write a C++ template function that takes two
arguments of the same type and returns their
sum.*/

#include <iostream>
using namespace std;

template <typename T>
T maximum(T a,T b){
    T c;
    c = a + b;
    
    return c;

}

int main(){
    cout<<maximum(3,5)<<endl;
    cout<<maximum(4.0,5.0)<<endl;
    // cout<<maximum('z','a');
    
    return 0;
} 
